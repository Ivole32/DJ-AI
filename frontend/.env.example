# Frontend Server Configuration
# Copy this file to .env and adjust values as needed

# Server Settings
PORT=3000
HOST=localhost
NODE_ENV=development

# Debug Mode
DEBUG=false
LOG_REQUESTS=false

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CORS_ENABLED=true
CORS_ORIGIN=*

# Logging
LOG_FILE=
